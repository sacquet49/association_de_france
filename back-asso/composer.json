{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.5",
        "ext-iconv": "*",
        "friendsofsymfony/rest-bundle": "^2.7.4",
        "jms/serializer-bundle": "^2.4.4",
        "lexik/jwt-authentication-bundle": "^2.6.5",
        "nelmio/cors-bundle": "^1.5.6",
        "phpdocumentor/reflection-docblock": "^5.1",
        "sensio/framework-extra-bundle": "^5.5.5",
        "symfony/console": "^4.4.8",
        "symfony/expression-language": "^4.4.8",
        "symfony/flex": "^1.6.3",
        "symfony/framework-bundle": "^4.4.8",
        "symfony/maker-bundle": "^1.17",
        "symfony/orm-pack": "^1.0.8",
        "symfony/process": "5.0.8",
        "symfony/security": "^4.4.8",
        "symfony/yaml": "^4.4.8"
    },
    "require-dev": {
        "symfony/dotenv": "^4.4.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C35Y9YCEKE66VR3F7S9GXZEF",
            "allow-contrib": false
        }
    }
}
