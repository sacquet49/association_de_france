{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.5",
        "ext-iconv": "*",
        "friendsofsymfony/rest-bundle": "^3.0.3",
        "jms/serializer-bundle": "^3.7.0",
        "lexik/jwt-authentication-bundle": "^v2.8.0",
        "nelmio/cors-bundle": "^2.1.0",
        "phpdocumentor/reflection-docblock": "^5.2.1",
        "sensio/framework-extra-bundle": "^v5.6.1",
        "symfony/console": "^v5.1.5",
        "symfony/expression-language": "^v5.1.5",
        "symfony/flex": "^v1.9.4",
        "symfony/framework-bundle": "^v5.1.5",
        "symfony/maker-bundle": "^v1.21.1",
        "symfony/orm-pack": "^v1.2.0",
        "symfony/process": "v5.1.5",
        "symfony/security-bundle": "^v5.1.5",
        "symfony/yaml": "^v5.1.5"
    },
    "require-dev": {
        "symfony/dotenv": "^v5.1.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C35Y9YCEKE66VR3F7S9GXZEF",
            "allow-contrib": false
        }
    }
}
